<script>window.addEventListener('DOMContentLoaded', function() {
		jQuery(document).ready(function($){
				
            $('.text-ul.elementor-widget-theme-post-content .elementor-widget-container h2').each(function(index){
	            $(this).clone(true).appendTo('#quicklink ul');
			})	
            $('#quicklink :header').each(function(index){
				$(':lang(ru-RU) .quicklink-block span').html('Содержание статьи');
				$(':lang(en-GB) .quicklink-block span').html('Article content');
                $(this).replaceWith('<li><a href="#point-'+index+'">' + $(this).html() +'</a></li>')					
			})								
            $('.text-ul.elementor-widget-theme-post-content .elementor-widget-container h2').each(function(index){
				$(this).attr('id','point-'+index+'');  
			})						
				
			if ($(window).width() <= '767'){
				$("#quicklink").on("click","a", function (event) {
				event.preventDefault();
				let id  = $(this).attr('href'),
					top = $(id).offset().top-40;
				$('body,html').animate({scrollTop: top}, 800);
				event.stopPropagation();
			});
			} else {       
				$("#quicklink").on("click","a", function (event) {
					event.preventDefault();
					let adminPanel;
					if (document.getElementById('wpadminbar')) {
						adminPanel = document.getElementById('wpadminbar').clientHeight;
					} else {
						adminPanel = 0;
					}
					let id  = $(this).attr('href'),
						top = $(id).offset().top-(135+adminPanel);
					$('body,html').animate({scrollTop: top}, 800);
				});
			}    				
        });		
				
	});</script>
